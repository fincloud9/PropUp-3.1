# =============================================================================
# PropUp Configuration Template - Environment Variables
# Version: 3.1.0
# =============================================================================
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# -----------------------------------------------------------------------------
# APPLICATION CONFIGURATION
# -----------------------------------------------------------------------------
REACT_APP_NAME=PropUp
REACT_APP_VERSION=3.1.0
REACT_APP_ENVIRONMENT=development
REACT_APP_BACKEND_URL=http://localhost:8000
REACT_APP_API_VERSION=v1

# -----------------------------------------------------------------------------
# BLOCKCHAIN CONFIGURATION
# -----------------------------------------------------------------------------
# Polygon Network
REACT_APP_POLYGON_MAINNET_RPC=https://polygon-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
REACT_APP_POLYGON_AMOY_RPC=https://rpc-amoy.polygon.technology
REACT_APP_POLYGON_CHAIN_ID=137
REACT_APP_POLYGON_TESTNET_CHAIN_ID=80002

# Arbitrum Network (Future)
REACT_APP_ARBITRUM_RPC=https://arb-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
REACT_APP_ARBITRUM_TESTNET_RPC=https://arb-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# RPC Providers
REACT_APP_ALCHEMY_API_KEY=your_alchemy_api_key_here
REACT_APP_INFURA_PROJECT_ID=your_infura_project_id
REACT_APP_QUICKNODE_ENDPOINT=your_quicknode_endpoint

# Contract Addresses - Phase 1 (MVP)
REACT_APP_PROPERTY_NFT_CONTRACT=0x0000000000000000000000000000000000000000
REACT_APP_PROPERTY_REGISTRY_CONTRACT=0x0000000000000000000000000000000000000000
REACT_APP_COMPLIANCE_MANAGER_CONTRACT=0x0000000000000000000000000000000000000000
REACT_APP_TITLE_REGISTRY_CONTRACT=0x0000000000000000000000000000000000000000

# Contract Addresses - Phase 2 (Post-MVP)
REACT_APP_MARKETPLACE_CONTRACT=0x0000000000000000000000000000000000000000
REACT_APP_RENTAL_MANAGER_CONTRACT=0x0000000000000000000000000000000000000000
REACT_APP_INVESTMENT_TOKEN_CONTRACT=0x0000000000000000000000000000000000000000
REACT_APP_CROWDFUNDING_CONTRACT=0x0000000000000000000000000000000000000000
REACT_APP_LENDING_POOL_CONTRACT=0x0000000000000000000000000000000000000000
