REACT_APP_GOVERNANCE_CONTRACT=0x0000000000000000000000000000000000000000
REACT_APP_TREASURY_CONTRACT=0x0000000000000000000000000000000000000000

# -----------------------------------------------------------------------------
# WALLET INTEGRATION
# -----------------------------------------------------------------------------
# WalletConnect / Reown
REACT_APP_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id
# Get from: https://cloud.reown.com

# RainbowKit Configuration
REACT_APP_ENABLE_RAINBOW_KIT=true
